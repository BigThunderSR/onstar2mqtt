# Docker Environment Configuration for OnStar2MQTT
# Copy this file to .env.docker and fill in your actual values
# This format is specifically for Docker's --env-file flag
#
# NOTE: Do NOT use quotes around values for Docker compatibility

# Required OnStar credentials
ONSTAR_VIN=your_17_character_vin
ONSTAR_USERNAME=your_onstar_email
ONSTAR_PASSWORD=your_onstar_password
ONSTAR_TOTP=your_totp_secret_key
ONSTAR_PIN=your_4_digit_pin

# Required MQTT settings
MQTT_HOST=localhost
MQTT_USERNAME=your_mqtt_username
MQTT_PASSWORD=your_mqtt_password

# Optional settings
ONSTAR_DEVICEID=
TOKEN_LOCATION=
ONSTAR_SYNC=true
ONSTAR_REFRESH=1800000
ONSTAR_POLL_INTERVAL=6
ONSTAR_POLL_TIMEOUT=90
ONSTAR_ALLOW_COMMANDS=true
MQTT_PORT=1883
MQTT_TLS=false
MQTT_REJECT_UNAUTHORIZED=true
MQTT_PREFIX=homeassistant
MQTT_NAME_PREFIX=
MQTT_ONSTAR_POLLING_STATUS_TOPIC=
MQTT_LIST_ALL_SENSORS_TOGETHER=false
MQTT_CA_FILE=
MQTT_CERT_FILE=
MQTT_KEY_FILE=
LOG_LEVEL=info
